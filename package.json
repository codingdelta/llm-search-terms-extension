{
  "name": "llm-search-terms-plugin",
  "displayName": "LLM Search Terms Plugin",
  "version": "0.0.1",
  "description": "LLM Search Terms Plugin",
  "author": "us",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.7.2",
    "@plasmohq/storage": "^1.15.0",
    "@vueuse/core": "^11.3.0",
    "cheerio": "^1.1.0",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.1",
    "plasmo": "0.90.5",
    "tailwindcss": "3.4.1",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "stream-browserify": "^3.0.0",
    "typescript": "5.3.3"
  },
  "background": {
    "service_worker": "src/background",
    "type": "module"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "webRequest",
      "scripting"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "assets/interceptor.js"
        ],
        "matches": [
          "https://chatgpt.com/*"
        ]
      }
    ]
  },
  "packageManager": "pnpm@10.12.3"
}
